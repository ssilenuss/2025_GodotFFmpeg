config_version=5

[application]

config/name="FFmpeg Video Player Demo"
config/description="Following Voylin's GameDev Journey to create an FFmpeg-based video player in Godot"
config/version="2025.10.23"
config/use_custom_user_dir=true
config/custom_user_dir_name="FFmpegGodot"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[debug]

file_logging/enable_file_logging=true

[rendering]

renderer/rendering_method="mobile"
